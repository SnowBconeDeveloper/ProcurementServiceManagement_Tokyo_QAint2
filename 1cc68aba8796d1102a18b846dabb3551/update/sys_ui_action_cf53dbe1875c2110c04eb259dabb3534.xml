        <name>Submit POs</name>
        <onclick>submitPO()</onclick>
        <order>100</order>
        <script><![CDATA[function submitPO() {
    var entries = g_list.getChecked();
    if (!entries || entries.length == 0)
        return;
    else {
        var ga = new GlideAjax('sn_psm_sap_int.PsmSapUtil_ClientCallable');
        ga.addParam('sysparm_name', 'callSubflowPO');
        ga.addParam('sysparm_sysid_list', entries);
        ga.getXML(callback);

        function callback(response) {
            var answer = response.responseXML.documentElement.getAttribute("answer");
            alert(answer);
            location.reload();
        }
    }
}]]></script>
